package backjoon;

//import java.util.Scanner;

public class Snail {

	public static void main(String[] args) {
		int count = 1, m, startX, startY, stopX, stopY;
		int movedistance = 1;
//		Scanner scanner = new Scanner(System.in);
//		m = scanner.nextInt();
//		scanner.close();
		m = 5;
		int[][] box = new int[m][m];
		for(int i = 0; i < box.length ; i++) {
			for(int j = 0 ; j < box.length ; j++) {
				box[i][j] = 0;
			}
		}
		if(m/2==0) {
			startX = (m/2)-1;
			startY = (m/2)-1;
		}else {
			startX = m/2; 
			startY = m/2;
		}
		while(count <= m*m) 
		
		for(int i = 0 ; i < m; i++) {   // 2차원 배열 출력
			for(int j = 0 ; j < m ; j++) {
				System.out.println(box[i][j]+"");
			}
		}
	}
}
