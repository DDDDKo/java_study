package ch02_sub;

public class G_InterFaceSub {
	
	void method1 () {
		System.out.println(G_Message.GREETING_MESAAGE);
		
		System.out.println(G_Message.GOODBYE_MESAAGE);
	}
	
	void method2 () {
		System.out.println(G_Message.GREETING_MESAAGE);
		
		System.out.println(G_Message.GOODBYE_MESAAGE);
	}
}
